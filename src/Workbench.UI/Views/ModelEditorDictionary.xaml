<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:Workbench.ViewModels"
                    xmlns:local="clr-namespace:Workbench.Views">

    <DataTemplate DataType="{x:Type viewModels:SingletonVariableEditorViewModel}">
        <local:SingletonVariableEditorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:AggregateVariableEditorViewModel}">
        <local:AggregateVariableEditorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:DomainEditorViewModel}">
        <local:DomainEditorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:ExpressionConstraintEditorViewModel}">
        <local:ExpressionConstraintEditorView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:AllDifferentConstraintEditorViewModel}">
        <local:AllDifferentConstraintEditorView/>
    </DataTemplate>

</ResourceDictionary>
